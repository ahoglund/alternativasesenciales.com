    <div class="col-lg-6">
        <div class="panel panel-default">
            <div class="panel-heading">
                <i class="fa fa-bell fa-fw"></i> Last Sign Ups
            </div>
            <!-- /.panel-heading -->
            <div class="panel-body">
                <div class="list-group">
                    <% @last_signups.each do |user| %>
                    <%= link_to edit_admin_user_path(user), class: "list-group-item" do %>
                    <i class="fa fa-user fa-fw"></i> <%= user.email %>
                    <span class="pull-right text-muted small">
                        <em><%= time_ago_in_words(user.created_at)%> ago</em>
                    </span>
                    <% end %>
                    <% end %>
                </div> <!-- /.list-group -->
            </div> <!-- /.panel-body -->
        </div> <!-- /.panel -->
    </div> <!-- /.col -->